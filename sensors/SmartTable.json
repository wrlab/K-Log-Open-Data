{
  "id": "https://.../td/SmartTable",
  "@context": "https://schema.iot.webizing.org/",
  "@type": ["SmartTable"],
  "name": "SmartTable Sensor",
  "names": "",
  "description" : "음식 리스트, 음식 칼로리, 사진 URL 로깅",
  "properties": {
    "name": {
      "@type": "name",
      "type": "string",
      "title": "Name",
      "description": "name of the sensor - format: firstName+LastName ex)jonghoLee, wanhoIm",
      "forms": [
        {
          "href": "there should be url for access value of this property",
          "contentType": "application/json"
        }
      ],
      "readOnly": true
    },
    "user": {
      "@type": "schema:Text",
      "type": "string",
      "title": "User",
      "description": "user of the sensor - format: firstName+LastName ex)jonghoLee, wanhoIm",
      "forms": [{
          "href": "URL",
          "contentType": "application/json"
        }],
      "readOnly": true
    },
    "address": {
      "@type": "address",
      "type": "string",
      "title": "Address",
      "description": "building where the sensor resides - format: organization-building ex)yongjaelee-house, kist-l1, kist-l8",
      "forms": [{"href": "URL", "contentType": "application/json"}],
      "readOnly": true
    },
    "room": {
      "@type": "room",
      "type": "string",
      "title": "Room",
      "description": "room number of the building where the sensor resides - ex) L8321",
      "forms": [{"href": "URL", "contentType": "application/json"}],
      "readOnly": true
    },
    "location": {
      "@type": "location",
      "type": "string",
      "title": "Location",
      "description": "location of the sensor - format: free",
      "forms": [{"href": "URL", "contentType": "application/json"}],
      "readOnly": true
    },
    "time": {
      "@type": "time",
      "type": "string",
      "title": "Time",
      "description": "time at which the data measurement was made - ex)\"2017-05-30T18:54:20+09:00\"",
      "forms": [{"href": "URL", "contentType": "application/json"}],
      "readOnly": true
    },
    "foodList": {
      "@type": "foodList",
      "type": "array",
      "title": "FoodList",
      "description": "List of food items",
      "forms": [{"href": "URL", "contentType": "application/json"}],
      "readOnly": true
    },
    "caloryList": {
      "@type": "caloryList",
      "type": "array",
      "title": "CaloryList",
      "description": "List of calories for the food items",
      "forms": [{"href": "URL", "contentType": "application/json"}],
      "readOnly": true
    },
    "snapShot": {
      "@type": "snapShot",
      "type": "string",
      "title": "SnapShot",
      "description": "url of the snap shot of the table",
      "forms": [{"href": "URL", "contentType": "application/json"}],
      "readOnly": true
    }
  },
  "forms": [
    {
      "href": "there should be url for access value of this property",
      "mockup" : {
        "name": "Foobot00",
        "user": "jonghoLee",
        "address": "kist-l1",
        "room": "L8321",
        "location": "On the table",
        "time": "2017-05-30T18:54:20+09:00",
        "foodList": "[kimchi, rice, driedSeaWeed]",
        "caloryList": "[40kcal, 250kcal, 30kcal]",
        "snapShot": "https://schema.iot.webizing.org/pictures/20180826"
      },
      "contentType": "application/json",
      "op": ["readallproperties"]
    }
  ],
  "securityDefinitions": {"nosec_sc": {"scheme": "nosec"}},
  "security": ["nosec_sc"]
}
